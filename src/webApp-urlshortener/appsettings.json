{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "InMemoryUrlShortenerExpiryOperationalStore": {
    "defaultExpiredRedirectKey": "1",
    "records": [
      {
        "ExpiredRedirectKey": "1",
        "tenant": "marketing",
        "ExpiredRedirectUrl": "https://www.google.com"
      },
      {
        "ExpiredRedirectKey": "2",
        "tenant": "marketing",
        "ExpiredRedirectUrl": "https://www.twitter.com"
      },
      {
        "ExpiredRedirectKey": "3",
        "tenant": "marketing",
        "ExpiredRedirectUrl": "https://www.github.com"
      }
    ]
  },
  "jwt-validate-settings": "{\r\n    \"jwt-validation\": {\r\n      \"authority\": \"https://apim-organics.azure-api.net/oauth2\",\r\n\r\n      \"options\": {\r\n        \"validateAudience\": {\r\n          \"required\": true,\r\n          \"validAudiences\": [ \"url-shortener\" ]\r\n        },\r\n        \"validateIssuer\": {\r\n          \"required\": true,\r\n          \"validIssuer\": \"https://apim-organics.azure-api.net/oauth2\"\r\n        },\r\n        \"validateLifetime\": {\r\n          \"required\": true,\r\n          \"clockSkew-min\": 5\r\n        },\r\n        \"validateIssuerSigningKey\": {\r\n          \"required\": true\r\n        }\r\n      }\r\n    }\r\n  }"

}
